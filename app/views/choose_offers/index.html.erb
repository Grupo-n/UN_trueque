<p id="notice"><%= notice %></p>

<h1>Choose Offers</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @choose_offers.each do |choose_offer| %>
      <tr>
        <td><%= link_to 'Show', choose_offer %></td>
        <td><%= link_to 'Edit', edit_choose_offer_path(choose_offer) %></td>
        <td><%= link_to 'Destroy', choose_offer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Choose Offer', new_choose_offer_path %>
